<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <x:String x:Key="AppTitle">TranslateUI</x:String>
  <x:String x:Key="WelcomeText">Добро пожаловать в TranslateUI</x:String>
  <x:String x:Key="TranslateButton">Перевод</x:String>
  <x:String x:Key="SourceTextLabel">Исходный текст</x:String>
  <x:String x:Key="ResultTextLabel">Результат</x:String>
  <x:String x:Key="SourceTextPlaceholder">Введите текст для перевода...</x:String>
  <x:String x:Key="ResultTextPlaceholder">Здесь появится перевод</x:String>
